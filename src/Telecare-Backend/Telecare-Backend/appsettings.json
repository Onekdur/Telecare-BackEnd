{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "TelecareDb": "Server=.\\SQLEXPRESS;Database=Telecare;Trusted_Connection=True;TrustServerCertificate=True;",
    "TelecareLoggingDb": "Server=.\\SQLEXPRESS;Database=TelecareLogging;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "MSSqlServer",
